{
  "name": "parcel-reactjs",
  "version": "1.0.0",
  "description": "A minimal React.js boilerplate with parcel as bundler",
  "main": "index.js",
  "scripts": {
    "dev": "parcel ./src/index.html",
    "start": "parcel build ./src/index.html --public-url ./ && node server/",
    "serverTests": "mocha server/tests/*"
  },
  "keywords": [
    "parcel",
    "react"
  ],
  "engines": {
    "node": "8.11.2"
  },
  "author": "“Robin <“wrobin@gmx.net”> (https://www.robinwieruch.de)",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^1.0.0",
    "eslint-plugin-mocha": "^5.0.0",
    "express": "^4.16.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "socket.io": "^2.1.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "mocha": "^5.2.0",
    "parcel-bundler": "^1.8.1"
  }
}
